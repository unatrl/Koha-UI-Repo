#. %13$s:  biblio.author | replace('"', '""') | $raw 
#. %14$s: - delimiter | $raw -
#. %15$s:  (biblioitem.publicationyear || biblio.copyrightdate) | replace('"', '""') | $raw 
#. %16$s: - delimiter | $raw -
#. %17$s:  biblioitem.publishercode | replace('"', '""') | $raw 
#. %18$s: - delimiter | $raw -
#. %19$s:  AuthorisedValues.GetDescriptionByKohaField( kohafield => 'items.ccode', authorised_value => item.ccode ) | replace('"', '""') | $raw 
#. %20$s: - delimiter | $raw -
#. %21$s:  item.barcode | replace('"', '""') | $raw 
#. %22$s: - delimiter | $raw -
#. %23$s:  item.itemnumber | replace('"', '""') | $raw 
#. %24$s: - delimiter | $raw -
#. %25$s:  item.enumchron | replace('"', '""') | $raw 
#. %26$s: - delimiter | $raw -
#. %27$s:  item.itemcallnumber | replace('"', '""') | $raw 
#. %28$s: - delimiter | $raw -
#. %29$s:  Branches.GetName(item.homebranch) | replace('"', '""') | $raw 
#. %30$s: - delimiter | $raw -
#. %31$s:  Branches.GetName(item.holdingbranch) | replace('"', '""') | $raw 
#. %32$s: - delimiter | $raw -
#. %33$s:  AuthorisedValues.GetDescriptionByKohaField(frameworkcode => biblio.frameworkcode, kohafield => 'items.location', authorised_value => item.location) | replace('"', '""') | $raw 
#. %34$s: - delimiter | $raw -
#. %35$s:  ItemTypes.GetDescription(item.itype) | replace('"', '""') | $raw 
#. %36$s: - delimiter | $raw -
#. %37$s:  item.stocknumber | replace('"', '""') | $raw 
#. %38$s: - delimiter | $raw -
#. %39$s:  AuthorisedValues.GetDescriptionByKohaField(frameworkcode => biblio.frameworkcode, kohafield => 'items.notforloan', authorised_value => item.notforloan) | replace('"', '""') | $raw 
#. %40$s: - delimiter | $raw -

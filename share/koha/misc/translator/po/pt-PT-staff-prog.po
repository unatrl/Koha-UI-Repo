#. %42$s: - delimiter | $raw -
#. %43$s:  AuthorisedValues.GetDescriptionByKohaField( kohafield => 'items.withdrawn', authorised_value => item.withdrawn ) || "" | replace('"', '""') | $raw 
#. %44$s: - delimiter | $raw -
#. %45$s:  AuthorisedValues.GetDescriptionByKohaField( kohafield => 'items.damaged', authorised_value => item.damaged ) || "" | replace('"', '""') | $raw 
#. %46$s: - delimiter | $raw -
#. %47$s:  (item.issues || 0) | $raw 
#. %48$s: - delimiter | $raw -
#. %49$s:  item.datelastborrowed | $KohaDates | $raw 
#. %50$s: - delimiter | $raw -
